    "@babel/register": "^7.10.1",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "css-loader": "^2.1.0",
    "csv-parse": "^4.10.1",
    "encoding-japanese": "^1.0.30",
    "eslint": "^5.3.0",
    "imi-enrichment-address": "git+https://github.com/IMI-Tool-Project/imi-enrichment-address.git",
