  "main": "./bin/build.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config ./docs/webpack.config.js",
    "build": "webpack -p --config ./webpack.config.js",
    "build:api": "npm run build:clean && node ./bin/build.js",
    "build:clean": "rm -fr ./data && mkdir ./data && rm -fr ./docs/api && mkdir ./docs/api",
    "test": "npm run test:lint && npm run test:mocha ./test/**/*.spec.js",
    "test:lint": "eslint \"src/**/*.js\" --fix",
    "test:mocha": "mocha --require @babel/register",
    "test:addresses": "npm run test:mocha ./test/addresses.js",
    "download": "bash bin/download.sh",
    "tree": "gzcat cache/isj.txt.gz  | node bin/tree.js cache/sac_label.json cache/sac_parent.json cache/sac_change.json > src/imi-enrichment-address/lib/tree.json"
  },
